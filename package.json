{
  "name": "reconfig",
  "version": "0.0.1",
  "description": "a tool for building configurations",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test BABEL_DISABLE_CACHE=1 WEBPACK_CWD=`pwd` ava --verbose",
    "start": "NODE_ENV=development babel-node dev-server.js",
    "lint": "eslint ./src ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechau/reconfig.git"
  },
  "keywords": [
    "webpack",
    "eslint",
    "configuration",
    "react",
    "babel",
    "karma"
  ],
  "author": "Mike Chau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikechau/reconfig/issues"
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.9.0",
    "classnames": "^2.2.5",
    "json-loader": "^0.5.4",
    "lodash": "^4.12.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "webpack": "^1.13.0",
    "webpack-merge": "^0.12.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-plugin-module-alias": "^1.4.0",
    "babel-plugin-rewire": "^1.0.0-rc-3",
    "babel-plugin-transform-define": "^1.0.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.2",
    "eslint-config-mc": "^0.2.1",
    "eslint-import-resolver-babel-module-alias": "^1.0.4",
    "eslint-import-resolver-webpack": "^0.2.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "jsdom": "^9.1.0",
    "jsx-test-helpers": "^1.1.0",
    "react-addons-test-utils": "^15.1.0",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">= 5.8.0"
  },
  "ava": {
    "babel": {
      "babelrc": true,
      "plugins": [
        "rewire",
        [
          "transform-define", {
            "process.env.NODE_ENV": "test"
          }
        ],
        [
          "module-alias",
          [
            {
              "src": "./src",
              "expose": "app"
            }
          ]
        ],
        [
          "webpack-loaders",
          [
            {
              "config": "${WEBPACK_CWD}/webpack.config.js",
              "verbose": true
            }
          ]
        ]
      ]
    },
    "require": [
      "babel-register",
      "./test/helpers/set-up-browser-env.js"
    ]
  }
}
