{
  "presets": ["es2015", "stage-2", "react"],
  "plugins": [
    "transform-object-assign",
    "transform-class-properties",
    "transform-export-extensions",
    "react-hot-loader/babel"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
